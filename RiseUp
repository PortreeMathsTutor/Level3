using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class RiseUp : MonoBehaviour {

	// Use this for initialization
	void Start () {
		
	}
	
	// Update is called once per frame
	void FixedUpdate () {
        if (transform.position.y<1.5) { transform.position = new Vector3(transform.position.x,transform.position.y+0.01f,transform.position.z); }
	}
}
